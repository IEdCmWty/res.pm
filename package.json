		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@types/bun": "^1.1.17",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.30.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"mdsvex": "^0.11.2",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"svelte": "^5.19.0",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^4.0.12",
		"tailwindcss-animate": "^1.0.7",
