		"vite": "^5.4.11",
		"vitest": "^2.1.8"
	},
	"dependencies": {
		"@inlang/paraglide-sveltekit": "^0.11.5",
		"@tailwindcss/container-queries": "^0.1.1",
		"@sitis/config__tailwind": "*",
		"@sitis/tools__js": "*",
		"@sitis/ui__svelte": "*"
	}
